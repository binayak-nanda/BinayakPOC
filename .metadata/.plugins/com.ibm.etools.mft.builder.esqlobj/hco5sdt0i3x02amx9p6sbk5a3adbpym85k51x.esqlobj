CREATE FILTER MODULE P3_M6_MQMB_HDMPROXY_MSG_FLOW_Filter
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		    DECLARE FIELDNAME CHARACTER;
			SET FIELDNAME = FIELDNAME(Root. XMLNSC.*[<]);
		IF FIELDNAME = 'P3CustomerInfo' THEN
			-- Going to ASAP No HDM
			RETURN TRUE;
		END IF;
		IF FIELDNAME = 'HDMProxyRequest' THEN
			-- Going to HDM
			RETURN FALSE;
		END IF;  
				
	END;

END MODULE;