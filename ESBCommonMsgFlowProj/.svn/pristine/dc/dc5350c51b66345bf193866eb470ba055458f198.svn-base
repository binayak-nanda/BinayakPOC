DECLARE qbe NAMESPACE 'http://qbe.com.au/esb/schema/qbe/1';

DECLARE ERROR CONSTANT CHARACTER 'Error';
DECLARE WARNING CONSTANT CHARACTER 'Warning';
DECLARE INFO CONSTANT CHARACTER 'Info';

CREATE PROCEDURE getDEFAULT_NOT_SET() RETURNS CHARACTER BEGIN RETURN 'NotSet'; END;

CREATE PROCEDURE getERROR() RETURNS CHAR BEGIN RETURN ERROR; END;
CREATE PROCEDURE getWARNING() RETURNS CHAR BEGIN RETURN WARNING; END;
CREATE PROCEDURE getINFO() RETURNS CHAR BEGIN RETURN INFO; END;

